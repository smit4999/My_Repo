    </Window.DataContext>

    <Border Background="SeaGreen"
            CornerRadius="20">
        <DataGrid x:Name="Employ_Grid" AutoGenerateColumns="True" RowBackground="CadetBlue">
            
        </DataGrid>
    </Border>
</Window>
